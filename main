#include <conio.h>
#include <math.h>
int main()
{
  double a,b;
  int n;
  printf("Enter two numbers:");
  scanf ("%lf %lf", &a, &b);
  printf("Enter your choice:\n 1.+\n 2.-\n 3.*\n 4./\n");
  scanf ("%d", &n);
  switch(n)
  {
    case 1:
      printf("The Sum of %lf and %lf is %lf ",a,b,a+b);
      break;
     case 2:
      printf("The Subtraction of %lf and %lf is %lf",a,b,a-b);
      break;
    case 3:
       printf("The |Multiplication of %lf and %lf is %lf",a,b,a*b);
       break;
    case 4:
       if (b==0){
        printf("Invalid operation");
        }
       else
        printf("The division of %lf and %lf is %lf",a,b,a/b);
        break;
    default:
      printf("Invalid operation");
  }
  return 0;
}
